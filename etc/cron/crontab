PATH=/usr/local/bin:/usr/bin
SHELL=/usr/local/bin/bash

# Need this for `click` library
# See: http://click.pocoo.org/5/python3/
# This problem occurs only with crontab
LC_ALL=en_US.utf-8
LANG=en_US.utf-8

